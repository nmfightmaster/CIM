{
  "ConnectionStrings": {
    "CIMContext": "Data source=Data/CIM.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Libraries": [
    {
      "provider": "jsdelivr",
      "library": "jquery-ajax-unobtrusive@3.2.6",
      "destination": "wwwroot/lib/jquery-ajax-unobtrusive/"
    }
  ],
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://localhost:80"
      },
      "Https": {
        "Url": "http://localhost:443"
      }
    }
  }
}